{
  "Swagger": {
    "JsonRoute": "/swagger/{documentName}/swagger.yaml",
    "Description": "IOT Dash API",
    "UiEndpoint": "/swagger/v1/swagger.yaml",
    "Server": "http://localhost:8080",
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "IotDash.*": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "IotDash.Services.Evaluation.*": "Debug",
      "IotDash.Services.Evaluation.InterfaceEvaluator": "Debug",
      "Microsoft.*": "Warning"
    }
  },
  "AllowedHosts": "*"
}
